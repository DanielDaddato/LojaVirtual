@using Daddato.Lojavirtual.Web.HtmlHelpers
@model Daddato.Lojavirtual.Web.Models.ProdutosViewModel

    @foreach (var produto in Model.Produtos)
    {
        @Html.Partial("ProdutosDetalhes", produto);
    }
<div class="btn-group pull-right">
    @Html.PageLinks(Model.Paginacao, x => Url.Action("ListaProdutos", new { pagina = x, categoria = Model.CategoriaAtual }))
</div>
